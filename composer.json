{
    "name": "guszandy/laravel-tabler-bootstrap4",
    "type": "package",
    "description": "Laravel Package for integrating Tabler template and this package is Laravel Mix friendly.",
    "keywords": ["laravel", "tabler", "package", "composer", "admin", "dashboard", "bootstrap 4"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "laravel/framework": "5.8.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GusZandy\\Tabler\\Providers\\AppServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "GusZandy\\Tabler\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GusZandy\\Tabler\\Tests\\": "tests/"
        }
    }
}
